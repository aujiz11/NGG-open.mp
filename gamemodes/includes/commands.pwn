		{
			SendClientMessageEx(playerid, COLOR_GREY, "Ban khong the lam dieu nay bay gio.");
			return 1;
		}

		if(PlayerInfo[playerid][pJailTime] > 0)
		{
			SendClientMessageEx(playerid, COLOR_WHITE, "Ban khong the lam dieu nay trong tu.");
