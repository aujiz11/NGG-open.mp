						case 1: format(szMessage, sizeof(szMessage), "** VIP Dong %s: %s", GetPlayerNameEx(playerid), params);
						case 2: format(szMessage, sizeof(szMessage), "** VIP Bac %s: %s", GetPlayerNameEx(playerid), params);
