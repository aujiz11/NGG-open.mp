					SendClientMessageEx(iTargetID, COLOR_LIGHTBLUE, string);

					format(string, sizeof(string), "%s da moi %s tham gia %s", GetPlayerNameEx(playerid), GetPlayerNameEx(iTargetID), Businesses[iBusinessID][bName]);
