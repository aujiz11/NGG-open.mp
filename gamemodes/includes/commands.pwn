	            if(PlayerInfo[targetid][pFMember] == PlayerInfo[playerid][pFMember])
	            {
	                if(PlayerInfo[targetid][pRank] <= PlayerInfo[playerid][pRank])
	                {
	                    if(iDiv != PlayerInfo[targetid][pDivision])
	                    {
                            if(0 <= iDiv <= 5)
							{
								new file[32], month, day, year ;
