							}
						}
				    }
				}
			}
		}
		else if((0 <= strval(iVehicle) <= MAX_DYNAMIC_VEHICLES))
		{
		    new iDvSlotID = strval(iVehicle);
		    if(DynVehicleInfo[iDvSlotID][gv_iDisabled] && DynVehicleInfo[iDvSlotID][gv_igID] == iGroupID)
