				GivePlayerCash(giveplayerid, -fine);
			}

			foreach(new i: Player)
			{
				if(GetPVarInt(i, "pWatchdogWatching") == giveplayerid) {
					SendClientMessage(i, COLOR_WHITE, "You have stopped DM Watching.");
