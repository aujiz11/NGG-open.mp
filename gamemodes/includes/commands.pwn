                                SendClientMessageEx(p, COLOR_WHITE, string);
                            }
                        }
                        case 3:
                        {
                            if(PlayerInfo[p][pDonateRank] < 3) {
                                PlayerInfo[p][pPaintTokens] += 5;
                                format(string,sizeof(string),"Ban da duoc hoan tra tong cong %d Paintball Tokens vi dong cua som.",5);
                                SendClientMessageEx(p, COLOR_WHITE, string);
