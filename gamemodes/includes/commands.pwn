            }
        }
        else {
            SendClientMessageEx(playerid, COLOR_GRAD2, "Ban khai o trong turf moi co the tat turfs!");
        }
    }
    else {
        SendClientMessageEx(playerid, COLOR_GRAD2, "Ban phai la LEO Faction, ban khong the tat turfs!");
