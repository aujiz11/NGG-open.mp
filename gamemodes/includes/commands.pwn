		}
		if(PlayerCuffed[playerid] >= 1) {
			SendClientMessageEx(playerid, COLOR_WHITE, "Ban khong the lam dieu nay khi bi cong tay.");
			return 1;
		}

		new SpeedRadar = GetPVarInt(playerid, "SpeedRadar");
